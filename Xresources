! rxvt-unicode main options ---------------------------------------------------
URxvt.perl-ext-common:		default,matcher,resize-font
! URxvt.urlLauncher:		/usr/bin/chromium-dev
URxvt.url-launcher:		/usr/bin/firefox
! URxvt.url-launcher:		/home/katie/bin/ff-open
URxvt.matcher.button:		1
URxvt.matcher.rend.0:		fg16 RS_Uline

URxvt.urgentOnBell:		True

!URxvt*meta8:			false
!URxvt.keysym.C-Left:		\033[1;5D
!URxvt.keysym.C-Right:		\033[1;5C
!URxvt*keysym.Home:		\033[1~
!URxvt*keysym.End:		\033[4~

URxvt*keysym.C-j: command:\033]710;-*-fixed-medium-r-normal--10-*-*-*-*-*-*-*\007

URxvt.geometry:			90x30
URxvt.externalBorder:		0
URxvt.borderless: true
URxvt.internalBorder: 6
URxvt.buffered:			true
URxvt.background:		black
URxvt.foreground:		white
URxvt.cursorColor:		white
urxvt*cursorBlink:		true
URxvt.underlineColor:		red

urxvt*scrollTtyKeypress:        true
!jump to bottom (prompt) when tty gets new lines
urxvt*scrollWithBuffer:         false
!jump to bottom (prompt) on tty output
urxvt*scrollTtyOutput:          false
!save n lines of scrollback
urxvt*saveLines:                10000
!toggle scrollbar
*scrollBar:             false
!scrollbar styles: rxvt, plain, next or xterm
urxvt*scrollstyle:              plain
!scrollbar alignment
urxvt*scrollBar_right:          true
urxvt*title:                    terminal
!------------
!True Transparency
!------------
